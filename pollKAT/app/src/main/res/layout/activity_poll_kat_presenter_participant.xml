<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.amon.pollkat.PollKatPresenterParticipant">


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pollKatPresenterID"
        android:layout_marginTop="47dp"
        android:src="@mipmap/presenterbutton"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/pollKatParticipantID"
        android:layout_alignStart="@+id/pollKatParticipantID" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/pollKatParticipantID"
        android:layout_marginTop="44dp"
        android:src="@mipmap/paticipantbutton"
        android:onClick="onParticipant"
        android:layout_below="@+id/pollKatPresenterID"
        android:layout_centerHorizontal="true" />
</RelativeLayout>
